fun main(args: Array<String>) 
{
	var r=3
    var c=4
    var matrix=Array(r){Array(c){0}}
    
    for(j in 0 until r)
    {
    println("For row ${j+1} =")
    
    for(k in 0 until c)
    {
    print("Enter number = ")
    matrix[j][k]=readLine()!!.toInt()
    }
    }
   var matrix1=Array(c){Array(r){0}}
   
      for(j in 0 until r)
    {
       for(k in 0 until c)
    {
    matrix1[k][j]=matrix[j][k]
    }
    }
    
    println("Original matrix")
    
      for(j in 0 until r)
    {
       for(k in 0 until c)
    {
    print("${matrix[j][k]} \t")
    }
    println("")
    }
   println("Transpose matrix")
    
      for(j in 0 until c)
    {
       for(k in 0 until r)
    {
    print("${matrix1[j][k]} \t")
    }
    println("")
    }
}
